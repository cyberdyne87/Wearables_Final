<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.wearables_final">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher_spacesuit_round"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_launcher_spacesuit_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CandyCatcher">
        <activity
            android:name=".ConstellationDiscoverer"
            android:exported="true" />
        <activity
            android:name=".SpaceExplorer"
            android:exported="true" />
        <activity
            android:name=".FinalFrontier"
            android:exported="true" />
        <activity
            android:name=".EventHorizon"
            android:exported="true" />
        <activity
            android:name=".DodgeAndWeave"
            android:exported="true" />
        <activity
            android:name=".LunarLander"
            android:exported="true" />
        <activity
            android:name=".Trophy"
            android:exported="true" />
        <activity
            android:name=".Manual"
            android:exported="true" />
        <activity
            android:name=".Credits"
            android:exported="true" />
        <activity
            android:name=".GPSActivity"
            android:configChanges="orientation"
            android:exported="true"
            android:screenOrientation="portrait" />
        <activity
            android:name=".GameActivity"
            android:configChanges="orientation"
            android:exported="true"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MainActivity"
            android:configChanges="orientation"
            android:exported="true"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>